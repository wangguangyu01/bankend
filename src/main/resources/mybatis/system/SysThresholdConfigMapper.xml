<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.smart119.system.dao.SysThresholdConfigDao">



    <select id="selectPageVo" resultType="com.smart119.system.domain.SysThresholdConfigDo">
        select * from sys_threshold_config r order by r.id
    </select>

    <update id="changeStatus">
        update  sys_threshold_config set `status` = #{status} where `id` = #{id}
    </update>



</mapper>
