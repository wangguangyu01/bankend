<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.smart119.jczy.dao.WblxrDao">

	<select id="get" resultType="com.smart119.jczy.domain.WblxrDO">
		select `WBLXR_ID`,`XM`,`DH`,`DZ`,`SR`,`YX`,`BZ`,`XFJYRY_TYWYSBM`,`cdate`,`status`,`cperson` from txl_wblxr where WBLXR_ID = #{value}
	</select>

	<select id="list" resultType="com.smart119.jczy.domain.WblxrDO">
		select `WBLXR_ID`,`XM`,`DH`,`DZ`,`SR`,`YX`,`BZ`,`XFJYRY_TYWYSBM`,`cdate`,`status`,`cperson` from txl_wblxr
        <where>  
		  		  <if test="wblxrId != null and wblxrId != ''"> and WBLXR_ID = #{wblxrId} </if>
		  		  <if test="xm != null and xm != ''"> and XM = #{xm} </if>
		  		  <if test="dh != null and dh != ''"> and DH = #{dh} </if>
		  		  <if test="dz != null and dz != ''"> and DZ = #{dz} </if>
		  		  <if test="sr != null and sr != ''"> and SR = #{sr} </if>
		  		  <if test="yx != null and yx != ''"> and YX = #{yx} </if>
		  		  <if test="bz != null and bz != ''"> and BZ = #{bz} </if>
		  		  <if test="xfjyryTywysbm != null and xfjyryTywysbm != ''"> and XFJYRY_TYWYSBM = #{xfjyryTywysbm} </if>
		  		  <if test="cdate != null and cdate != ''"> and cdate = #{cdate} </if>
		  		  <if test="status != null and status != ''"> and status = #{status} </if>
		  		  <if test="cperson != null and cperson != ''"> and cperson = #{cperson} </if>
		  		</where>
        <choose>
            <when test="sort != null and sort.trim() != ''">
                order by ${sort} ${order}
            </when>
			<otherwise>
                order by WBLXR_ID desc
			</otherwise>
        </choose>
		<if test="offset != null and limit != null">
			limit #{offset}, #{limit}
		</if>
	</select>
	
 	<select id="count" resultType="int">
		select count(*) from txl_wblxr
		 <where>  
		  		  <if test="wblxrId != null and wblxrId != ''"> and WBLXR_ID = #{wblxrId} </if>
		  		  <if test="xm != null and xm != ''"> and XM = #{xm} </if>
		  		  <if test="dh != null and dh != ''"> and DH = #{dh} </if>
		  		  <if test="dz != null and dz != ''"> and DZ = #{dz} </if>
		  		  <if test="sr != null and sr != ''"> and SR = #{sr} </if>
		  		  <if test="yx != null and yx != ''"> and YX = #{yx} </if>
		  		  <if test="bz != null and bz != ''"> and BZ = #{bz} </if>
		  		  <if test="xfjyryTywysbm != null and xfjyryTywysbm != ''"> and XFJYRY_TYWYSBM = #{xfjyryTywysbm} </if>
		  		  <if test="cdate != null and cdate != ''"> and cdate = #{cdate} </if>
		  		  <if test="status != null and status != ''"> and status = #{status} </if>
		  		  <if test="cperson != null and cperson != ''"> and cperson = #{cperson} </if>
		  		</where>
	</select>
	 
	<insert id="save" parameterType="com.smart119.jczy.domain.WblxrDO">
		insert into txl_wblxr
		(
			`WBLXR_ID`, 
			`XM`, 
			`DH`, 
			`DZ`, 
			`SR`, 
			`YX`, 
			`BZ`, 
			`XFJYRY_TYWYSBM`, 
			`cdate`, 
			`status`, 
			`cperson`
		)
		values
		(
			#{wblxrId}, 
			#{xm}, 
			#{dh}, 
			#{dz}, 
			#{sr}, 
			#{yx}, 
			#{bz}, 
			#{xfjyryTywysbm}, 
			#{cdate}, 
			#{status}, 
			#{cperson}
		)
	</insert>
	 
	<update id="update" parameterType="com.smart119.jczy.domain.WblxrDO">
		update txl_wblxr 
		<set>
			<if test="xm != null">`XM` = #{xm}, </if>
			<if test="dh != null">`DH` = #{dh}, </if>
			<if test="dz != null">`DZ` = #{dz}, </if>
			<if test="sr != null">`SR` = #{sr}, </if>
			<if test="yx != null">`YX` = #{yx}, </if>
			<if test="bz != null">`BZ` = #{bz}, </if>
			<if test="xfjyryTywysbm != null">`XFJYRY_TYWYSBM` = #{xfjyryTywysbm}, </if>
			<if test="cdate != null">`cdate` = #{cdate}, </if>
			<if test="status != null">`status` = #{status}, </if>
			<if test="cperson != null">`cperson` = #{cperson}</if>
		</set>
		where WBLXR_ID = #{wblxrId}
	</update>
	
	<delete id="remove">
		delete from txl_wblxr where WBLXR_ID = #{value}
	</delete>
	
	<delete id="batchRemove">
		delete from txl_wblxr where WBLXR_ID in 
		<foreach item="wblxrId" collection="array" open="(" separator="," close=")">
			#{wblxrId}
		</foreach>
	</delete>

